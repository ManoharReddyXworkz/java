package com.xworkz.corejava.Constructor.Defaultconstructor;

public class Defaultconstructor {
public static void main(String[]args) {

	Gun gun =new Gun(4,1.5,"Black");
	Gun gun1 = new Gun(6,2,"Gray");
	
	System.out.println("Gun Detaials:");
	System.out.println("noofBullet:"+gun.noofBullet);
	System.out.println("Weight:"+gun.weight);
    System.out.println("color:"+gun.colour );
    System.out.println("Gun Detaials:");
    System.out.println("noofBullet:"+gun.noofBullet);
    System.out.println("weight: "+gun1.weight);
    System.out.println("color:"+gun1.colour);
    
    		
}
}
